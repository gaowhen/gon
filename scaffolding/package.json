{
  "name": "gon",
  "version": "0.1.0",
  "scripts": {
    "lint-staged": "lint-staged",
    "precommit": "lint-staged",
    "commitmsg": "./.commit-msg.js",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0"
  },
  "lint-staged": {
    "*.js": [
      "eslint",
      "git add"
